FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default     steadyState;
}

gradSchemes
{
    default         leastSquares;//Gauss linear;
}

divSchemes
{
    default         none;
    div(phi,C_)	    Gauss linear;//
}

laplacianSchemes
{
    default         Gauss linear corrected;//none;
    //laplacian(ks,fi)  Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
}

